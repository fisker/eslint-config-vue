{
  "name": "@xwtec/eslint-config-vue",
  "version": "6.0.5",
  "description": "ESLint Configuration for xwtec vue projects",
  "keywords": [
    "config",
    "eslint",
    "vue",
    "xwtec"
  ],
  "homepage": "https://github.com/xwtec/eslint-config-vue#readme",
  "bugs": {
    "url": "https://github.com/xwtec/eslint-config-vue/issues"
  },
  "repository": "xwtec/eslint-config-vue",
  "license": "MIT",
  "author": {
    "name": "xwtec",
    "url": "https://github.com/xwtec"
  },
  "files": [
    "configs",
    "index.js"
  ],
  "sideEffects": false,
  "main": "index.js",
  "scripts": {
    "build": "npm-run-all build:*",
    "build:rules": "node -r esm ./scripts/rules",
    "dist": "run-p dist:*",
    "dist:npm": "np --yolo --no-yarn",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "prettier **/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml} --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint **/*.{js,mjs,vue}",
    "lint:markdown": "markdownlint **/*.md --ignore \"**/node_modules/**\"",
    "lint:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --check",
    "release": "run-s build test format lint dist",
    "test": "ava"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "require": [
      "esm"
    ],
    "verbose": true
  },
  "dependencies": {
    "@xwtec/eslint-config": "6.0.21",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-vue": "6.0.1",
    "vue-eslint-parser": "7.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "8.2.0",
    "@fisker/commitlint-config": "1.1.4",
    "@fisker/husky-config": "1.1.5",
    "@fisker/lint-staged-config": "1.0.8",
    "@xwtec/eslint-config": "6.0.21",
    "@xwtec/prettier-config": "1.0.3",
    "ava": "2.4.0",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "6.7.2",
    "esm": "3.2.25",
    "husky": "3.1.0",
    "lint-staged": "9.5.0",
    "markdownlint-cli": "0.19.0",
    "mem": "6.0.1",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "write-prettier-file": "1.2.3"
  },
  "peerDependencies": {
    "eslint": "*",
    "prettier": "*"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
