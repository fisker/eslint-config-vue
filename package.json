{
  "name": "@xwtec/eslint-config-vue",
  "version": "4.1.9",
  "description": "ESLint Configuration for xwtec vue projects",
  "main": "index.js",
  "keywords": [
    "xwtec",
    "eslint",
    "config"
  ],
  "files": [
    "index.js"
  ],
  "scripts": {
    "release": "run-s lint build test prettier dist",
    "dist": "run-p dist:*",
    "dist:npm": "np --no-yarn --no-cleanup",
    "test": "eslint **/*.js --config ./index.js",
    "prettier": "prettier **/*.{js,md,json} --write",
    "lint": "run-p lint:*",
    "lint:js": "eslint **/*.js --fix",
    "lint:markdown": "markdownlint *.md",
    "build": "npm-run-all build:*",
    "build:rules": "node -r esm ./scripts/rules"
  },
  "license": "MIT",
  "dependencies": {
    "@xwtec/eslint-config": "^5.0.4",
    "eslint-plugin-vue": "^5.2.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": "xwtec/eslint-config-vue",
  "devDependencies": {
    "@commitlint/cli": "^7.6.0",
    "@fisker/commitlint-config": "^1.0.5",
    "@fisker/lint-staged-config": "^1.0.4",
    "@xwtec/prettier-config": "^1.0.2",
    "cz-conventional-changelog-emoji": "^0.1.0",
    "eslint": "^5.16.0",
    "esm": "^3.2.22",
    "husky": "^2.2.0",
    "lint-staged": "^8.1.6",
    "markdownlint-cli": "^0.15.0",
    "mem": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.17.0"
  },
  "peerDependencies": {
    "eslint": "*",
    "prettier": "*"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog-emoji"
    }
  },
  "bugs": {
    "url": "https://github.com/xwtec/eslint-config-vue/issues"
  },
  "homepage": "https://github.com/xwtec/eslint-config-vue#readme",
  "author": {
    "name": "xwtec",
    "url": "https://github.com/xwtec"
  },
  "prettier": "@xwtec/prettier-config"
}
